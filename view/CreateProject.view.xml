														<core:Item text="{path: 'factorCatalog>key', formatter: '.formatter.translate'}" key="{factorCatalog>key}" class="selectClientTechnology"/>
													</Select>
													<SegmentedButton id="segmentedButtonClientTechnology" selectedKey="{factorCatalog>currentWeight}" select="_onSelectionChange"
