												<!-- Dont need columns header, as upper table has already defined them. -->
												<Column ></Column>
												<Column ></Column>
												<Column ></Column>
											</columns>
											<ColumnListItem>
												<cells>
													<HBox width="auto" justifyContent="SpaceBetween">
														<ObjectIdentifier title="{path: 'factorCatalog>factor', formatter: '.formatter.translate'}"/>
