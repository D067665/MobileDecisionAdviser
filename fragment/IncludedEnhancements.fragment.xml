				<Text text="{i18n>GER02}" wrapping="true" editable="false"/>
				<Text text="{i18n>GER03}" wrapping="true" editable="false"/>
				<Text text="{i18n>GER04}" wrapping="true" editable="false"/>
				<Text text="{i18n>GER05}" wrapping="true" editable="false"/>
				<Text text="{i18n>GER06}" wrapping="true" editable="false"/>
				<Text text="{i18n>GER07}" wrapping="true" editable="false"/>
