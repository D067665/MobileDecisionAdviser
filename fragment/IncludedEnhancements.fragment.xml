<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:mvc="sap.ui.core.mvc" xmlns="sap.m" xmlns:l="sap.ui.layout"
	xmlns:f="sap.ui.layout.form" controllerName="M4A.controller.CreateProject" xmlns:html="http://www.w3.org/1999/xhtml">
	<Dialog title="Included Enhancements">
		<l:Grid defaultSpan="L12 M12 S12" width="auto">
			<l:content>
				
				<Title text="{i18n>IncludedEnhancements}" titleStyle="H2"/>
